            else if (param1.eventid == 7) {
                if ((this.gotglyph == 1) && (param1.args[2] == scslosifoundation::stringtoidentifier("glyp"))) {
                    if (gctoa::glyphdone(globalclass.player.actor.toahandle) == 1) {
