class strt2_puzzle {
    property parent;
    property gatestone;
    property door;
    property upgradefruit3;
    property upgradefruit3_shown;
    
    method strt2_puzzle(param1) {
        this.parent = param1;
        return null;
    }
    
    method initfull() {
        var x;
        var y;
        var z;
        var tohunga;
        var hive1handle;
        var hive2handle;
        gcmodeldirector::setnearestmax(20);
        gcareadirector::setnearestmax(20);
        gcareadirector::setfog(-9143565, 0 - 1000.0, 0 - 2400.0, 0.01);
        gclightdirector::setcavelighting(1);
        globalclass.bgmusic.changefilename("Root/Data/sounds/MusicGaliAmbience");
        globalclass.bgmusic.play();
        this.door = new lego_moveableobject("drs0");
        this.door.initfull();
        hive1handle = gcareadirector::gethive(scslosifoundation::stringtoidentifier("ghv0"));
        gchive::setmaxcreatures(hive1handle, 8);
        gchive::setcreaturetype(hive1handle, scslosifoundation::stringtoidentifier("crb1"));
        gchive::setspawnrate(hive1handle, 5);
        hive2handle = gcareadirector::gethive(scslosifoundation::stringtoidentifier("ghv1"));
        gchive::setmaxcreatures(hive2handle, 8);
        gchive::setcreaturetype(hive2handle, scslosifoundation::stringtoidentifier("crb1"));
        gchive::setspawnrate(hive2handle, 5);
        tohunga = gcmodeldirector::getcharacter(scslosifoundation::stringtoidentifier("vlgr"), 0);
        gccharacter::settohunga(tohunga, rgba(0, 0, 255, 255), rgba(0, 0, 255, 255), scslosifoundation::stringtoidentifier("mkaa"));
        gccharacter::setinputstate(tohunga, scslosifoundation::stringtoidentifier("none"));
        gccharacter::setsplitanimation(tohunga, scslosifoundation::stringtoidentifier("d003"), -1);
        gccharacter::setorientation(tohunga, -56);
        if (gcsaver::finditem(scslosifoundation::stringtoidentifier("gstn")) == 0) {
            this.gatestone = new lego_pickup("gstn", 0, 5, "item", 7, 1);
            this.gatestone.initfull();
            this.gatestone.show();
            this.gatestone.setcondition(262144);
            this.gatestone.setquest(3);
        }
        this.upgradefruit3_shown = gcsaver::finditem(scslosifoundation::stringtoidentifier("fru1"));
        if (this.upgradefruit3_shown == 0) {
            this.upgradefruit3 = new vitality_pickup("fru1", 17, 2, "frut", 1, 1);
            this.upgradefruit3.initfull();
        }
        if (bettersaver::getbooleanvalue(37) == 1) {
            tohunga = gcmodeldirector::getcharacter(scslosifoundation::stringtoidentifier("vlgr"), 0);
            gccharacter::settohunga(tohunga, rgba(0, 0, 255, 255), rgba(0, 0, 255, 255), scslosifoundation::stringtoidentifier("mkaa"));
            gccharacter::setinputstate(tohunga, scslosifoundation::stringtoidentifier("none"));
            gccharacter::switchanimation(tohunga, scslosifoundation::stringtoidentifier("d000"), -1);
            x = this.door.getx();
            y = this.door.gety() + 9.0;
            z = this.door.getz();
            this.door.interpolate(x, y, z, 0.0);
        }
        return null;
    }
    
    method initshort() {
        globalclass.player.actor.gotopos(this.parent.enterstartid, this.parent.enterlookid);
        globalclass.player.unkill();
        return null;
    }
    
    method cleanupfull() {
        if (this.gatestone != null) {
            this.gatestone.cleanupfull();
            this.gatestone = null;
        }
        if (this.upgradefruit3 != null) {
            this.upgradefruit3.cleanupfull();
            this.upgradefruit3 = null;
        }
        if (this.door != null) {
            this.door.cleanupfull();
            this.door = null;
        }
        return null;
    }
    
    method process(param1) {
        return null;
    }
    
    method onevent(param1) {
        if (param1.eventid == 41) {
            if (bettersaver::getbooleanvalue(37)) {
                this.give_fruit3();
                this.movedoorup();
            }
        }
        else if (param1.eventid == 46) {
            gccollisionphysicsgroup::spinmask(0.7);
        }
        else if (param1.eventid == 27) {
            this.initshort();
        }
        return null;
    }
    
    method input(param1) {
        if (param1.button == 90) {
            if (param1.event == 2) {
                this.movedoorup();
            }
        }
        if (param1.button == 88) {
            if (param1.event == 2) {
                this.cheat();
            }
        }
        return null;
    }
    
    method cheat() {
        var var1;
        var1 = new scriptevent(51, [ 0, 
            1 ]);
        var1.send();
        return null;
    }
    
    method movedoorup() {
        var x;
        var y;
        var z;
        x = this.door.getx();
        y = this.door.gety() + 9.0;
        z = this.door.getz();
        this.door.interpolate(x, y, z, 1.5);
        return null;
    }
    
    method exit() {
        this.parent = null;
        return null;
    }
    
    method give_fruit3() {
        if (this.upgradefruit3_shown == 0) {
            this.upgradefruit3.obj.setpos(48.23598, 1.150006, -34.39081);
            this.upgradefruit3_shown = 1;
        }
        return null;
    }
}