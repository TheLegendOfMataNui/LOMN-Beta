class lego_area_mdmn : gs_lego_area {
    property puzzle;
    
    method lego_area_mdmn(param1) {
        this.gs_lego_area("mdmn", param1, 0, 0);
        this.puzzle = new mdmn_puzzle(this);
        this.lego_area_initfull();
        return null;
    }
    
    method initfull() {
        this.lego_area_show();
        this.puzzle.initfull();
        return null;
    }
    
    method initshort() {
        globalclass.bgmusic.stop();
        return null;
    }
    
    method process(param1) {
        this.puzzle.process(param1);
        return null;
    }
    
    method input(param1) {
        this.puzzle.input(param1);
        return null;
    }
    
    method onevent(param1) {
        this.puzzle.onevent(param1);
        return null;
    }
    
    method endarea() {
        globalclass.bgmusic.changefilename("Root/Data/Sounds/OnuaCompletionMusic");
        return null;
    }
    
    method cleanupfull() {
        this.lego_area_hide();
        this.puzzle.cleanupfull();
        return null;
    }
    
    method exit() {
        this.lego_area_cleanupfull();
        if (this.puzzle != null) {
            this.puzzle.exit();
            this.puzzle = null;
        }
        return null;
    }
}