class lego_area_clf2 : gs_lego_area {
    property puzzle;
    
    method lego_area_clf2(param1) {
        this.gs_lego_area("clf2", param1, 0, 0);
        this.puzzle = new cliff2_puzzle(this);
        this.lego_area_initfull();
        return null;
    }
    
    method initfull() {
        this.lego_area_show();
        gcareadirector::setnearestmax(20);
        gcmodeldirector::setnearestmax(20);
        gcareadirector::setfog(-402183, 0 - 30.0, 0 - 1200.0, 0.05);
        gclightdirector::setcavelighting(0);
        this.puzzle.initfull();
        globalclass.bgmusic.changefilename("Root/Data/sounds/MusicOnuaAmbience");
        globalclass.bgmusic.play(1);
        return null;
    }
    
    method onevent(param1) {
        this.puzzle.onevent(param1);
        return null;
    }
    
    method cleanupfull() {
        this.lego_area_hide();
        this.puzzle.cleanupfull();
        return null;
    }
    
    method exit() {
        this.lego_area_cleanupfull();
        if (!(this.puzzle == null)) {
            this.puzzle.exit();
            this.puzzle = null;
        }
        return null;
    }
}