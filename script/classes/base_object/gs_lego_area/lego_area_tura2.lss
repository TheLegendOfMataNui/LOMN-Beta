class lego_area_tura2 : gs_lego_area {
    
    method lego_area_tura2(param1) {
        this.gs_lego_area("tura", param1, 0, 0);
        this.lego_area_initfull();
        return null;
    }
    
    method initfull() {
        this.lego_area_show();
        gcmodeldirector::setnearestmax(3);
        gcareadirector::setnearestmax(3);
        gcareadirector::setfog(-9143565, 0 - 1000.0, 0 - 2400.0, 0.01);
        gclightdirector::setcavelighting(1);
        if (gcsaver::portalactive() == 1) {
            gccollisionphysicsgroup::setportal(scslosifoundation::stringtoidentifier("tps9"), "root/data/art/portals/gali/shrine_ripple/g_shrn_ptl", 1);
        }
        else {
            gccollisionphysicsgroup::turnoffnextarea(scslosifoundation::stringtoidentifier("tps9"));
        }
        globalclass.bgmusic.changefilename("Root/Data/sounds/MusicVillage");
        globalclass.bgmusic.play(1);
        return null;
    }
    
    method process(param1) {
        return null;
    }
    
    method input(param1) {
        return null;
    }
    
    method onevent(param1) {
        if (param1.eventid == 4) {
            if (param1.args[0] == scslosifoundation::stringtoidentifier("tps9")) {
                screenlist.screens[2].screen.areas[12].puzzle.needstobeunderwater(0);
            }
        }
        return null;
    }
    
    method cleanupfull() {
        this.lego_area_hide();
        return null;
    }
    
    method exit() {
        this.lego_area_cleanupfull();
        return null;
    }
}