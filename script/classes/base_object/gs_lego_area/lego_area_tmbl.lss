class lego_area_tmbl : gs_lego_area {
    property puzzle;
    
    method lego_area_tmbl(param1) {
        this.gs_lego_area("tmbl", param1, 0, 0);
        this.lego_area_initfull();
        this.puzzle = new tumblemazepuzzlep(this);
        return null;
    }
    
    method initfull() {
        this.lego_area_show();
        this.puzzle.initfull();
        gcareadirector::setnearestmax(5);
        gcareadirector::setfog(-16777216, 0 - 1.0, 0 - 60.0, 0.05);
        gclightdirector::setcavelighting(1);
        globalclass.bgmusic.changefilename("Root/Data/sounds/MUSICPart2LegoUNDERGROUND");
        globalclass.bgmusic.play(1);
        this.puzzle.initfull();
        return null;
    }
    
    method process(param1) {
        this.puzzle.process(param1);
        return null;
    }
    
    method input(param1) {
        this.puzzle.input(param1);
        return null;
    }
    
    method onevent(param1) {
        this.puzzle.onevent(param1);
        return null;
    }
    
    method cleanupfull() {
        this.lego_area_hide();
        this.puzzle.cleanupfull();
        return null;
    }
    
    method exit() {
        this.puzzle.exit();
        this.puzzle = null;
        return null;
    }
}