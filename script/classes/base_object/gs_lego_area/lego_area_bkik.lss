class lego_area_bkik : gs_lego_area {
    property puzzle;
    
    method lego_area_bkik(param1) {
        this.gs_lego_area("bkik", param1, 0, 0);
        this.puzzle = new bkik_puzzle(this);
        this.lego_area_initfull();
        this.startingarea = [ 180, 
            -356, 
            0 - 51.4, 
            689 ];
        return null;
    }
    
    method initfull() {
        this.puzzle.initfull();
        this.puzzle.show();
        globalclass.enemy.killheight = -52;
        return null;
    }
    
    method process(param1) {
        this.puzzle.process(param1);
        return null;
    }
    
    method onevent(param1) {
        this.puzzle.onevent(param1);
        return null;
    }
    
    method input(param1) {
        this.puzzle.input(param1);
        return null;
    }
    
    method cleanupfull() {
        this.puzzle.cleanupfull();
        return null;
    }
    
    method exit() {
        this.puzzle = null;
        return null;
    }
}