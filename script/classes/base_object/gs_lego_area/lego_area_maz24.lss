class lego_area_maz24 : gs_lego_area {
    property puzzle;
    property parent;
    property swordhook;
    
    method lego_area_maz24(param1) {
        this.parent = param1;
        this.gs_lego_area("maz2", param1, 0, 0);
        this.puzzle = new l4_maz2_puzzle(this);
        this.lego_area_initfull();
        return null;
    }
    
    method initfull() {
        this.lego_area_show();
        this.puzzle.initfull();
        globalclass.bgmusic.changefilename("Root/Data/sounds/MUSICLEGOUNDERGROUND");
        globalclass.bgmusic.play(1);
        if (gcsaver::finditem(scslosifoundation::stringtoidentifier("swrd")) == 0) {
            this.swordhook = new lego_pickup("swrd", 0, 3, "item", 9, 1);
            this.swordhook.initfull();
            this.swordhook.show();
        }
        return null;
    }
    
    method cleanupfull() {
        this.lego_area_hide();
        this.puzzle.cleanupfull();
        return null;
    }
    
    method onevent(param1) {
        this.puzzle.onevent(param1);
        return null;
    }
    
    method input(param1) {
        this.puzzle.input(param1);
        return null;
    }
    
    method process(param1) {
        this.puzzle.process(param1);
        return null;
    }
    
    method exit() {
        if (!(this.puzzle == null)) {
            this.puzzle.exit();
            this.puzzle = null;
        }
        if (!(this.swordhook == null)) {
            this.swordhook.cleanupfull();
            this.swordhook = null;
        }
        return null;
    }
}