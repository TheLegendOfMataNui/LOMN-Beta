class lego_area_brd34 : gs_lego_area {
    property puzzle;
    
    method lego_area_brd34(param1) {
        this.gs_lego_area("brd3", param1, 0, 0);
        this.puzzle = new brd3_puzzle(this);
        this.lego_area_initfull();
        return null;
    }
    
    method initfull() {
        this.lego_area_show();
        this.puzzle.initfull();
        this.puzzle.show();
        globalclass.bgmusic.changefilename("Root/Data/sounds/MusicSurfAndSnowboard");
        globalclass.bgmusic.play(1);
        return null;
    }
    
    method cleanupfull() {
        this.lego_area_hide();
        this.puzzle.hide();
        this.puzzle.cleanupfull();
        this.puzzle = null;
        return null;
    }
    
    method process(param1) {
        this.puzzle.process(param1);
        return null;
    }
    
    method onevent(param1) {
        this.puzzle.onevent(param1);
        return null;
    }
    
    method input(param1) {
        this.puzzle.input(param1);
        return null;
    }
}