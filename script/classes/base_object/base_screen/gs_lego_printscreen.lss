class gs_lego_printscreen : base_screen {
    property menu;
    property menuobject;
    property selected;
    property printthingy;
    property gobackflag;
    
    method gs_lego_printscreen(param1) {
        return null;
    }
    
    method initfull() {
        this.menu = [ "Tahu poster (1 page)", 
            "Tahu poster (4 page)", 
            "Tahu action scene", 
            "Tahu concept art", 
            "Tahu concept art 2", 
            "Lewa poster (8 page)", 
            "Lewa action scene", 
            "Lewa concept art", 
            "Wind moster concept art", 
            "BACK" ];
        this.menuobject = new gs_lego_printscreenmenu(this.menu);
        this.menuobject.initfull();
        this.printthingy = scprinter::create();
        this.gobackflag = 0;
        return null;
    }
    
    method cleanupfull() {
        this.menu = null;
        if (!(this.menuobject == null)) {
            this.menuobject.cleanupfull();
            this.menuobject = null;
        }
        return null;
    }
    
    method process(param1) {
        if (this.gobackflag) {
            screenlist.backward();
            this.gobackflag = 0;
        }
        return null;
    }
    
    method onevent(param1) {
        return null;
    }
    
    method input(param1) {
        this.menuobject.input(param1);
        if (param1.button == 13) {
            if (param1.event == 1) {
                this.selectobject();
            }
        }
        if (param1.button == 32) {
            if (param1.event == 1) {
                this.selectobject();
            }
        }
        return null;
    }
    
    method selectobject() {
        var var1;
        var var2;
        this.selected = 1;
        var1 = 0;
        gcstaticsoundsptrarray::playtablesound(1074);
        if (this.menuobject.pointer == 0) {
            var2 = scprinter::printfile(this.printthingy, "tp", "Tahu Poster (1 page)", 1);
        }
        else if (this.menuobject.pointer == 1) {
            var2 = scprinter::printfile(this.printthingy, "tp", "Tahu Poster (4 page)", 4);
        }
        else if (this.menuobject.pointer == 2) {
            var2 = scprinter::printfile(this.printthingy, "ta", "Tahu action scene", 1);
        }
        else if (this.menuobject.pointer == 3) {
            var2 = scprinter::printfile(this.printthingy, "tc.jpg", "Tahu concept art", 1);
        }
        else if (this.menuobject.pointer == 4) {
            var2 = scprinter::printfile(this.printthingy, "tm.jpg", "Tahu concept art 2", 1);
        }
        else if (this.menuobject.pointer == 5) {
            var2 = scprinter::printfile(this.printthingy, "lp", "Lewa poster (8 page)", 8);
        }
        else if (this.menuobject.pointer == 6) {
            var2 = scprinter::printfile(this.printthingy, "la", "Lewa action scene", 1);
        }
        else if (this.menuobject.pointer == 7) {
            var2 = scprinter::printfile(this.printthingy, "lc.jpg", "Lewa concept art", 1);
        }
        else if (this.menuobject.pointer == 8) {
            var2 = scprinter::printfile(this.printthingy, "lm.jpg", "Wind monster concept art", 1);
        }
        else if (this.menuobject.pointer == 9) {
            this.menuobject.hideall();
            this.gobackflag = 1;
        }
        return null;
    }
}