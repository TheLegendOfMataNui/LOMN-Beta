class base_drawobject : base_object {
    property x1;
    property x2;
    property y1;
    property y2;
    property z;
    property w;
    property h;
    property transval;
    property onscreen;
    
    method base_drawobject(x1_pos, x2_pos, y1_pos, y2_pos) {
        if (x1_pos != null) {
            this.x1 = x1_pos;
        }
        else {
            this.x1 = 0;
        }
        if (y1_pos != null) {
            this.y1 = y1_pos;
        }
        else {
            this.y1 = 0;
        }
        if (x2_pos != null) {
            this.x2 = x2_pos;
        }
        else {
            this.x2 = 0;
        }
        if (y2_pos != null) {
            this.y2 = y2_pos;
        }
        else {
            this.y2 = 0;
        }
        this.w = this.x2 - this.x1;
        this.h = this.y2 - this.y1;
        return null;
    }
    
    method add() {
        return null;
    }
    
    method show() {
        this.onscreen = 1;
        return null;
    }
    
    method hide() {
        this.onscreen = 0;
        return null;
    }
    
    method remove() {
        return null;
    }
    
    method enablealphachannel(param1) {
        return null;
    }
    
    method enabletransparency(param1) {
        return null;
    }
    
    method settransparentvalue(param1) {
        return null;
    }
}