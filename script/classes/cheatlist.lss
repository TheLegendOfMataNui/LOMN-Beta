                }
                this.curidx;
            }
            else if (param1.button == 36) {
                if (this.curidx == 0) {
                    globalclass.bgmusic.play();
                }
                else if (this.curidx == 1) {
                    this.jukeboxmenusetup();
                }
            }
            else if (param1.button == 46) {
