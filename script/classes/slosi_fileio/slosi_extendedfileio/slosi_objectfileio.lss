class slosi_objectfileio : slosi_extendedfileio {
    
    method slosi_objectfileio(filename) {
        this.filename = filename + ".osd";
        return null;
    }
    
    method saveosiobject(object) {
        scslosifileio::saveosiobject(this.filename, object);
        return null;
    }
    
    method loadosiobject() {
        return scslosifileio::loadosiobject(this.filename);
    }
}