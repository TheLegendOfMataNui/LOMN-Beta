                this.give_fruit2();
            }
        }
        else if (param1.eventid == 3) {
            if (param1.args[0] == scslosifoundation::stringtoidentifier("tb00")) {
                if (param1.args[1] == 1) {
                    globalclass.player.canshootdragger(0);
                }
                else if (param1.args[1] == 0) {
                    globalclass.player.canshootdragger(1);
                }
            }
            param1.args[0];
        }
        return null;
    }
    
    method input(param1) {
        if (param1.button == 90) {
            if (param1.event == 2) {
